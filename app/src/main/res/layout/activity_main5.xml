<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.phone1000.admin.ecook.activity.Main5Activity">
    <RelativeLayout
        android:id="@+id/rl_dindan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff">
        <ImageButton
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:paddingLeft="10dp"
            android:layout_width="30dp"
            android:onClick="onClick"
            android:layout_height="wrap_content"
            android:src="@mipmap/go_back_normal"
            android:background="#ffffffff"/>
        <TextView
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:paddingRight="10dp"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="全部订单"
            android:textSize="20sp"/>
    </RelativeLayout>
    <android.support.design.widget.TabLayout
        android:layout_below="@id/rl_dindan"
        android:id="@+id/shoucang_tabLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </android.support.design.widget.TabLayout>
    <View
        android:id="@+id/shoucang_View"
        android:layout_below="@id/shoucang_tabLayout"
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <android.support.v4.view.ViewPager
        android:layout_below="@id/shoucang_View"
        android:id="@+id/shoucang_ViewPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v4.view.ViewPager>
</RelativeLayout>
